void setup() {
  FastLED.addLeds<WS2812B, DATA_PIN_FIRST, GRB>(leds0, NUM_LEDS);
  FastLED.addLeds<WS2812B, DATA_PIN_SECOND, GRB>(leds90, NUM_LEDS);
  FastLED.addLeds<WS2812B, DATA_PIN_THIRD, GRB>(leds180, NUM_LEDS);
  FastLED.addLeds<WS2812B, DATA_PIN_FOURTH, GRB>(leds270, NUM_LEDS);
  pinMode(HALL_PIN, INPUT_PULLUP);
  attachInterrupt(digitalPinToInterrupt(HALL_PIN), hallSignal, FALLING);
}

void hallSignal() {
  lastRevTime = revTime;
  revTime = millis();
  turning = true;
}

byte mapPixelToLed(int led) {
  x = 18 + ((int)(((cos8(currentAngle >> 8) - 128) * (led  + 2.333) * zoomX)) >> 7);
  y = 18 - ((int)(((sin8(currentAngle >> 8) - 128) * (led  + 2.333) * zoomY)) >> 7);
  y = constrain(y, 1, 35);
  return (pgm_read_byte(&imageProGlove[(((y - 1) * imageWidth + (x - 1)) + currentFrame ) % sizeOfImage]));
}

void setStrip(CRGB strip[NUM_LEDS], uint16_t stripAngle) {
  currentAngle = ((drawTime * maxAngle) / revPeriod) + stripAngle + angleOffset;
  currentAngle = maxAngle - (currentAngle % maxAngle);
  for (int i = 0; i < NUM_LEDS; i++) {
    byte pixelValue = mapPixelToLed(i);
    if ((currentFrame + x) % imageWidth < 98) {
      sat = pixelValue;
      val = max(40, sat);
    }
    else {
      sat = 0;
      val = pixelValue > 0 ? 0 : 40;
    }
    strip[i].setHSV(11, sat, val);
  }
}

void loop() {
  mainTime = millis();
  revPeriod = revTime - lastRevTime;
  drawTime = mainTime - revTime;
  if (mainTime - revTime > timeOutInterval) {        //timeout(stop detection)
    turning = false;
  }
  if (mainTime - lastFrameTime > timeBetweenFrames) {     //change between frames
    currentFrame = (currentFrame + 1) % imageWidth;
    lastFrameTime = mainTime;
  }
  if (turning) {   
    //Set First Strip (0째)
    setStrip(leds0, 8192);   
    //Set Second Strip (90째)
    setStrip(leds90, 24576);   
    //Set Third Strip (180째)
    setStrip(leds180, 40960);   
    //Set Fourth Strip (270째)
    setStrip(leds270, 57344);   
  }
  //turn off all strips while not turning
  else if (!turning) {
    for (int i = 0; i < NUM_LEDS; i++) {
      leds0[i].setHSV(0, 0, 0);
      leds90[i].setHSV(0, 0, 0);
      leds180[i].setHSV(0, 0, 0);
      leds270[i].setHSV(0, 0, 0);
    }
  }
  FastLED.show();
}

